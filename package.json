{
  "name": "trivia-game-mp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "hookrouter": "^1.2.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "5.0.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-node-logger": "^0.1.0",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "styled-components": "^4.4.1",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test",
    "test:watch": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "test:e2e": "./node_modules/cypress/bin/cypress run",
    "test:e2e:watch": "./node_modules/cypress/bin/cypress open",
    "test:all": "npm run test && npm run test:e2e",
    "code:clean": "yarn code:lint && yarn code:format",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\" --fix",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}' --write",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "predeploy": "npm run build",
    "deploy": "netlify deploy --prod",
    "deploy:draft": "netlify deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged && npm test && npm run test:e2e"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run code:clean",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/hookrouter": "^2.2.1",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.12",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.4.0",
    "@types/testing-library__cypress": "^5.0.1",
    "cross-env": "^6.0.3",
    "cypress": "^3.6.1",
    "eslint-plugin-cypress": "^2.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "poetic": "^1.1.0",
    "ts-jest": "^24.2.0",
    "whatwg-fetch": "^3.0.0"
  }
}
